# Pipecat Python Server Environment Variables

# Google Gemini API Key for LLM
GOOGLE_API_KEY=your_google_api_key_here

# Convex Configuration
CONVEX_HTTP_URL=https://your-deployment.convex.cloud
PIPECAT_SERVER_SECRET=your_shared_secret_key_here

# Optional: Pipecat Cloud API Keys (if using Pipecat Cloud)
# PIPECAT_API_KEY_PUBLIC=your_public_key_here
# PIPECAT_API_KEY_PRIVATE=your_private_key_here

# Optional: Daily.co API Key (if using Daily.co for WebRTC transport)
# DAILY_API_KEY=your_daily_api_key_here

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8000
